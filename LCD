
void setup()
{
  pinMode(2,OUTPUT);
  pinMode(8,OUTPUT);
  pinMode(9, INPUT);
  Serial.begin(9600);
 }

 void loop()
 {
  digitalWrite(8,HIGH);
  delayMicroseconds(10);
  digitalWrite(8, LOW);
 
 int distance=pulseIn(9,HIGH)*340/2/10000;
 Serial.print(distance);
 Serial.println("cm");
 delay(1000);
 
if(distance <= 150)
{
digitalWrite(2,HIGH);
delay(100);
digitalWrite(2,LOW);
delay(1000);
}}
